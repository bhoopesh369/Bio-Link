{
	"name": "Biolink-frontend",
	"version": "1.0.0",
	"description": "Client Repository of Aracadia",
	"scripts": {
		"lint": "eslint --ext .js,.jsx,.ts,.tsx .",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|jsx|tsx|json)\"",
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview"
	},
	"lint-staged": {
